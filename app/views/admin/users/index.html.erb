<div class="container">
  <div class="row">
    <div class="col-md-12 offset-md-3">
      <% @users.each do |user| %>
        <div class="rounded pb-4 pr-4">
          <%#= image_tag post.get_image(180,180) %>
          <%= link_to admin_user_path(user) do %>
          <%= user.nickname %>
          <% end %>
          <%= user.name_kana %><br>
      <% end %>
    </div>
  </div>
</div>